{
    "presets": ["es2015", "react", "stage-0"],
    "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [ {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }]
          }]
      ]
    },
    "test": {
        "sourceMaps": "inline",
        "plugins": [ ["istanbul", {
          "exclude": [
            "**/*-test.js*",
            "**/*-test-chrome.js*"
          ]
        }]
     ]
    }
  }
}
